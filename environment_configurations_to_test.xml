<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Environment config tsts" parallel="tests"
	thread-count="1">

	<listeners>
		<listener class-name="integrationTests.Listeners"></listener>
	</listeners>

	<test name="windows chrome">
		<parameter name="operating_system" value="windows" />
		<parameter name="browser" value="chrome" />
		<parameter name="browser_headless" value="no" />
		<classes>
			<class name="integrationTests.Runner" />
		</classes>
	</test>

